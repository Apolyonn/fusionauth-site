<ul>
  {% assign languages = site.data.exampleapps | group_by: "language" |sort: 'name' %}
  {% for lang in languages %}
    <li>
       <a href='/docs/v1/tech/example-apps/{{ lang.name }}'>{{lang.name}}</a>
    <ul>
      {% for app in lang.items %}
        <li>
          <a href="{{ app.url }}">
            {{ app.name }}
          </a> - {{app.description}}
        </li>
      {% endfor %}
    </ul>
  {% endfor %}
</ul>
